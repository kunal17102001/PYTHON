for n in range(101):
    if n is magicnumber:
        print("the magic no. is ",n)
        break
    else:
        print(n) 

Jersey_numbers=[12,9,3,8,17]
print("the jersey no's which are still available are : ")
for n in range(1,20):
    if n in Jersey_numbers:
        continue
    else:
        print(n)
        
        
